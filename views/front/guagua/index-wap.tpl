<html lang="en"><head>    <meta charset="UTF-8">    <meta name="viewport" content="width=device-width, initial-scale=1.0">    <meta http-equiv="X-UA-Compatible" content="ie=edge">    <title>{{.siteName}}</title>    <link rel="stylesheet" href="{{static_front}}/static/front/guagua/wap/css/base.css">    <link rel="stylesheet" href="{{static_front}}/static/front/guagua/wap/css/style.css"></head><body style="font-size: 12px;"><!-- 头部开始 --><div class="header" id="header">    <a href="{{urlfor "GuaGuaApiController.Get"}}" class="logo"></a>    <a href="{{urlfor "DoorController.Query" "gid" .gameId}}" class="search"></a></div><!-- 头部结束 --><div class="top">    <div class="bg"></div>    <div class="amount"><img src="{{static_front}}/static/front/guagua/wap/images/amount8668.png" width="100%" height="80"alt=""/></div>    <div class="prize kuangao">        <div class="kaishi1" id="demoCanvas">            <!--刮奖部分 -->            <div class="main_box">                <!-- 点击开始按钮 -->                <button id="kaishi" class="btn-search" type="submit" name="button"></button>                <div class="box1" id="guajiang">                    <div class="content" id="lotty">                        <div id="mask_img_bg">未登录</div>                        <img id="redux" src="{{static_front}}/static/front/guagua/wap/images/gao4.jpg"/>                    </div>                </div>            </div>        </div>    </div>        <!-- 剩余抽奖次数和再来一次 -->    <div  style="clear: both;height:20px;"></div>            <div class="num">                <p>剩余刮奖次数：<b id="guaCount">{{.lotteryNums}}</b><span>次</span></p>                <a href="javascript:void(0)" onclick="onceAgain1()">再来一次</a>            </div>            <!--登录后显示 -->            <div class="logined" style="{{if eq .loginAccount ""}}display:none;{{end}}">                <span>会员<b id="username">{{.loginAccount}}</b></span><a href="javascript:;" onclick="exit()" class="btn">退出</a>            </div>            <!-- 登录前显示-->            <div class="search" style="{{if .loginAccount}}display:none;{{end}}">                <input type="hidden" id="gid" value="{{.gameId}}">                <input type="hidden" id="check" value="{{.loginAccount}}">                <input type="text" id="account" value="" placeholder="请输入您的会员账号">                <button id="login"></button>            </div>        </div>    <!--显示活动内容 -->    <div class="info">        <div class="title"></div>        <div class="hd"><img src="{{static_front}}/static/front/guagua/wap/images/hd1.png" alt=""></div>        <div class="box">        {{str2html .gameRule}}        </div>    </div>    <div class="info" >        <div class="title"></div>        <div class="hd"><img src="{{static_front}}/static/front/guagua/wap/images/hd2.png" alt=""></div>        <div class="box">        {{str2html .gameStatement}}        </div>    </div></div><!--页面结束 --><script type="text/javascript" src="{{static_front}}/static/front/js/jquery.min.js"></script><script type="text/javascript" src="{{static_front}}/static/front/layer-v3.1.1/layer.js"></script><script type="text/javascript" src="{{static_front}}/static/front/guagua/wap/js/jquery.eraser.js"></script><script type="text/javascript" src="{{static_front}}/static/front/js/broadcast.js"></script><script type="text/javascript">    //点击开始    $("#kaishi").click(function () {        if ($("#check").val() == "") {            layer.msg("请登录后再开始刮奖！~~~");            return        }        scratchoff()    });    function scratchoff() {        $.post('{{urlfor "LotteryController.Lottery"}}', {                    gid: $("#gid").val(),                    account: $("#check").val()                },                function (obj) {                    switch (obj.code) {                        case 0:                            localStorage.clear();                            layer.msg(obj.msg);                            break;                        case 1:                            $("#kaishi").hide();                            $("#lotty").css("visibility","visible");                            $("#mask_img_bg").text(obj.data.gift);                            $('#guaCount').html($('#guaCount').text()-1);                            break;                        case 2:                            $("#kaishi").hide();                            $("#lotty").css("visibility","visible");                            $('#guaCount').html($('#guaCount').text()-1);                            var s = obj.msg;                            if (s =="谢谢参与，再接再厉"){                                h ="谢谢参与"                                $("#mask_img_bg").text(h);                                break                            }                            $("#mask_img_bg").text(s);                        default:                            break;                    }                });    }</script><!-- 登录验证--><script type="text/javascript">    $("#login").click(function () {        if ($("#account").val() == "") {            layer.msg("会员账号不能为空");            return        }        login();    });    function login() {        $.ajax({            url: '/login',            dataType: 'json',            cache: false,            type: 'POST',            data: {gid:$("#gid").val(),account: $("#account").val()},            success: function (obj) {                switch(obj.code){                    case 0:                        layer.msg(obj.msg);                        break;                    case 1:                        _username = $("#account").val();                        localStorage.username =_username;                        $('.logined').fadeIn();                        setTimeout(function () {                            location.href = location.href;                        }, 100);                        break;                    default:                        break;                }            },            error: function(XMLHttpRequest, textStatus, errorThrown) {                var x = 1;                localStorage.clear();            }        })    }    //再来一次    function onceAgain1() {        if ($("#check").val() == "") {            layer.msg("请登录后再开始刮奖！~~~");            return        }else if($('#guaCount').text()=="0"){            layer.msg("您没有活动参与资格或者已经参与过了哦~")            return        }        var gid = $("#gid").val();        location.href = "{{urlfor "GuaGuaApiController.get"}}" + "?gid=" + gid;    }</script><!--退出 --><script type="text/javascript">    function exit() {        $.ajax({            url: '/logout',            dataType: 'json',            cache: false,            type: 'GET',            success: function (obj) {                localStorage.clear();                window.location.reload();            },            error: function (XMLHttpRequest, textStatus, errorThrown) {                var x = 1;            }        })    }</script><script type="text/javascript" src="{{static_front}}/static/front/guagua/wap/js/respond.min.js"></script></div></body></html>